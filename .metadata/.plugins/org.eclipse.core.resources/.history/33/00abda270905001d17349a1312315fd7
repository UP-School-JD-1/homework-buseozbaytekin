package org.javaturk.oopj.hw3;

public class Operators {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int i = 1;
		
		System.out.println(i + 3);
		
		int y = i++;
		
		System.out.println(y);
		
		
		
		int b = 1; 
		
		int x = ++b;
		
		System.out.println(x);
		
		
		int number = 10;
		int number1 = 0;
		
		number1 += number;
	}

}
